{
	"name": "fetch data from APIpipeline",
	"properties": {
		"activities": [
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.Links",
						"type": "Expression"
					},
					"isSequential": false,
					"activities": [
						{
							"name": "CopyFromAPI2Blob",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "RestSource",
									"httpRequestTimeout": "00:01:40",
									"requestInterval": "00.00:00:00.010",
									"requestMethod": "GET",
									"paginationRules": {
										"AbsoluteUrl": "$.info.next"
									}
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobStorageWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"mappings": [
										{
											"source": {
												"path": "['id']"
											},
											"sink": {
												"name": "id",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['name']"
											},
											"sink": {
												"name": "name",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['status']"
											},
											"sink": {
												"name": "status",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['species']"
											},
											"sink": {
												"name": "species",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['type']"
											},
											"sink": {
												"name": "type",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['gender']"
											},
											"sink": {
												"name": "gender",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['origin']['name']"
											},
											"sink": {
												"name": "origin_name",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['origin']['url']"
											},
											"sink": {
												"name": "origin_url",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['location']['name']"
											},
											"sink": {
												"name": "location_name",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['location']['url']"
											},
											"sink": {
												"name": "location_url",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['image']"
											},
											"sink": {
												"name": "image",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['url']"
											},
											"sink": {
												"name": "character_url",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['created']"
											},
											"sink": {
												"name": "created",
												"type": "String"
											}
										}
									],
									"collectionReference": "$['results']"
								}
							},
							"inputs": [
								{
									"referenceName": "RestResourcecart",
									"type": "DatasetReference",
									"parameters": {
										"item": {
											"value": "@item()",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "webactoutputdes",
									"type": "DatasetReference",
									"parameters": {
										"filename": {
											"value": "@concat(item(),'.csv')",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"Links": {
				"type": "array",
				"defaultValue": [
					"users",
					"products"
				]
			}
		},
		"annotations": []
	}
}